{
  "name": "serafina-bot",
  "version": "2.0.0",
  "description": "A production-ready Discord bot with modular architecture, slash commands, and advanced features",
  "main": "dist/bot.js",
  "scripts": {
    "start": "node dist/bot.js",
    "dev": "ts-node-dev --respawn --transpile-only src/bot.ts",
    "build": "tsc --project tsconfig.json",
    "clean": "rimraf dist coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prebuild": "npm run clean",
    "postbuild": "npm run type-check",
    "register:commands": "ts-node src/scripts/register-commands.ts",
    "register:guild-commands": "ts-node src/scripts/register-commands.ts --guild",
    "clear:commands": "ts-node src/scripts/clear-commands.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:make-migration": "knex migrate:make",
    "docker:build": "docker build -t serafina-bot .",
    "docker:run": "docker run -d --name serafina-bot serafina-bot",
    "docker:stop": "docker stop serafina-bot && docker rm serafina-bot"
  },
  "keywords": [
    "discord",
    "bot",
    "gaming",
    "community",
    "automation",
    "typescript",
    "discord.js"
  ],
  "author": "NovaSanctum",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.11.0",
    "chalk": "^5.5.0",
    "cors": "^2.8.5",
    "discord.js": "^14.21.0",
    "express": "^4.21.2",
    "ioredis": "^5.3.2",
    "moment": "^2.30.1",
    "mongoose": "^8.1.1",
    "node-fetch": "^2.7.0",
    "openai": "^4.26.0",
    "p-limit": "^6.2.0",
    "tsconfig-paths": "^4.1.0",
    "tsx": "^4.20.3",
    "winston": "^3.17.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@aws-amplify/auth": "^6.13.3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.2.0",
    "@notionhq/client": "^4.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/supertest": "^2.0.16",
    "@types/webpack-env": "^1.18.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "aws-amplify": "^6.15.3",
    "date-fns": "^4.1.0",
    "dexie": "^4.0.11",
    "dotenv": "^17.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "framer-motion": "^12.23.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "react-router-dom": "^7.6.3",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "workbox-background-sync": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0",
    "workbox-window": "^7.3.0",
    "zustand": "^5.0.6"
  },
  "engines": {
