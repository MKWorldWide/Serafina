{
  "name": "serafina-bot",
  "version": "2.0.0",
  "description": "A production-ready Discord bot with modular architecture, slash commands, and advanced features",
  "main": "dist/index.js",
  "engines": {
    "node": ">=20 <21"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "node build.js",
    "start": "node dist/index.js",
    "register": "tsx src/registry/register.ts",
    "doctor": "tsx scripts/repo-doctor.ts",
    "migrate:db": "tsx scripts/migrate-db.ts",
    "migrate:sqlite-to-pg": "tsx scripts/migrate-sqlite-to-pg.ts",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "postinstall": "npm run build"
  },
  "keywords": [
    "discord",
    "bot",
    "gaming",
    "community",
    "automation",
    "typescript",
    "discord.js"
  ],
  "author": "NovaSanctum",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.11.0",
    "chalk": "^5.5.0",
    "cors": "^2.8.5",
    "discord.js": "^14.21.0",
    "dotenv-flow": "^3.3.0",
    "express": "^4.19.0",
    "fs-extra": "^11.2.0",
    "node-cron": "^3.0.3",
    "pg": "^8.11.0",
    "pino": "^9.0.0",
    "pino-pretty": "^10.2.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rimraf": "^6.0.0",
    "sql.js": "^1.10.3",
    "ws": "^8.16.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@aws-amplify/auth": "^6.15.0",
    "@types/express": "^4.17.23",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.11.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3",
    "zustand": "^5.0.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NovaSanctum/GameDinDiscord.git"
  },
  "bugs": {
    "url": "https://github.com/NovaSanctum/GameDinDiscord/issues"
  },
