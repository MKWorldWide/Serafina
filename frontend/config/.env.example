# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key

# Amplify Configuration
AMPLIFY_APP_ID=your_amplify_app_id
AMPLIFY_BRANCH=main
AMPLIFY_ENV=prod

# API Configuration
REACT_APP_API_ENDPOINT=your_api_endpoint
REACT_APP_WEBSOCKET_URL=your_websocket_url
REACT_APP_PERSONALIZE_TRACKING_ID=your_tracking_id

# Authentication
COGNITO_USER_POOL_ID=your_user_pool_id
COGNITO_CLIENT_ID=your_client_id
COGNITO_IDENTITY_POOL_ID=your_identity_pool_id

# Feature Flags
REACT_APP_ENABLE_PERSONALIZATION=true
REACT_APP_ENABLE_SOCIAL_FEATURES=true
REACT_APP_ENABLE_ACHIEVEMENTS=true

# Monitoring
REACT_APP_SENTRY_DSN=your_sentry_dsn
REACT_APP_LOGROCKET_ID=your_logrocket_id

# Third-party Services
REACT_APP_STRIPE_PUBLIC_KEY=your_stripe_public_key
REACT_APP_LAUNCH_DARKLY_CLIENT_ID=your_launch_darkly_client_id

# Cache Configuration
ELASTICACHE_PRIMARY_ENDPOINT=your_elasticache_endpoint
ELASTICACHE_PORT=6379

# Security
WAF_ACL_ARN=your_waf_acl_arn
SHIELD_PROTECTION_ID=your_shield_protection_id

# Testing
CYPRESS_BASE_URL=http://localhost:3000
JEST_COVERAGE_THRESHOLD=80 